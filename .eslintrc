{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-unused-vars": 2,
      "no-unreachable": 2,
      "no-var": 2,
      "prefer-const": 2,
      "semi": 0,
      "quotes": 2,
      "no-alert": 2
    }
  }